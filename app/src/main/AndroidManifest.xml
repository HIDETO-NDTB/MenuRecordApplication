<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ossan_programer.menurecordapplication">

    <application
        android:name="com.ossan_programer.menurecordapplication.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.ossan_programer.menurecordapplication.CalendarActivity"></activity>
        <activity android:name="com.ossan_programer.menurecordapplication.MenuListsActivity" />
        <activity android:name="com.ossan_programer.menurecordapplication.MenuListActivity" />
        <activity android:name="com.ossan_programer.menurecordapplication.MenuActivity" />
        <activity android:name="com.ossan_programer.menurecordapplication.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>